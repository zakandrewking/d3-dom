{
  "name": "tinier-dom",
  "version": "0.2.0",
  "description": "React-style DOM declarations for Tinier",
  "main": "lib/tinier-dom.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "test": "./node_modules/.bin/mocha --compilers js:babel-register src/tests/*.js || true",
    "test-debug": "./node_modules/.bin/mocha debug --compilers js:babel-register src/tests/*.js || true",
    "compile": "./node_modules/.bin/babel -d lib/ --ignore tests/* -s true src/ && ./node_modules/.bin/uglifyjs --compress --source-map lib/tinier-dom.min.js.map --in-source-map lib/tinier-dom.js.map -o lib/tinier-dom.min.js -- lib/tinier-dom.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zakandrewking/tinier-dom.git"
  },
  "author": "Zachary King",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zakandrewking/tinier-dom/issues"
  },
  "homepage": "https://github.com/zakandrewking/tinier-dom#readme",
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.4.3",
    "chai": "^3.4.1",
    "jsdom": "^8.0.2",
    "mocha": "^2.3.4",
    "uglify-js": "^2.7.3"
  }
}

{
  "name": "tinier-dom",
  "version": "0.2.0",
  "description": "React-style DOM declarations for Tinier",
  "main": "lib/main.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --compilers js:babel-register src/tests/*.js || true",
    "test-debug": "./node_modules/.bin/mocha debug --compilers js:babel-register src/tests/*.js || true",
    "node-inspector": "./node_modules/.bin/node-inspector",
    "test-node-inspector": "./node_modules/.bin/mocha --debug-brk --compilers js:babel-register src/tests/*.js || true",
    "compile": "./node_modules/.bin/babel -d lib/ --ignore tests/* -s true src/",
    "compilew": "./node_modules/.bin/babel -d lib/ --ignore tests/* -w -s true src/",
    "bundle": "./node_modules/.bin/webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zakandrewking/tinier-dom.git"
  },
  "author": "Zachary King",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zakandrewking/tinier-dom/issues"
  },
  "homepage": "https://github.com/zakandrewking/tinier-dom#readme",
  "dependencies": {
    "lodash.keyby": "^4.2.0",
    "lodash.mapvalues": "^4.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.4.3",
    "chai": "^3.4.1",
    "jsdom": "^8.0.2",
    "mocha": "^2.3.4",
    "node-inspector": "^0.12.7",
    "webpack": "^1.12.10"
  }
}
